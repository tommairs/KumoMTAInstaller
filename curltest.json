# or run `sh curltest.json`


curl -H 'Content-Type: application/json' 'http://127.0.0.1:8000/api/inject/v1' -d '{
    "envelope_sender": "'$EMAIL'",
    "content": "Subject: hello\n\nHello there",
    "recipients": [
        {
            "email": "'$EMAIL'"
        }
    ]
}'


